{
  "extends": "airbnb-base",
  "rules": {
    "linebreak-style": [2, "windows"],
  
    "no-unused-vars": [
      1,
      {
        "vars": "all",
        "args": "none"
      }
    ],
    "quotes": ["error", "double", {
        "avoidEscape": true,
        "allowTemplateLiterals": true
    }
  ],
    "no-cond-assign": [2, "always"]
   
  }
}
